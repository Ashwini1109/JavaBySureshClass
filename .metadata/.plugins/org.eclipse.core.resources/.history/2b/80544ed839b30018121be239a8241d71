//method hiding
class P{
	 void m1(){
		System.out.println("m parent");
	}
}
class C extends P{
	 void m1(){
		System.out.println("m child");
	}
}
public class MethodHiding {
     public static void main(String[] args) {
		P p=new  P();
		p.m1();
		C c=new C();
		c.m1();
		P p2=new C();
		p2.m1();
	}
}
